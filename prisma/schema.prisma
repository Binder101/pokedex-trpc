import {DATABASE_URL} from '../.env';

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Pokemon {
  id      Int      @id
  name    String   @unique
  types   String[]
  sprite  String
}